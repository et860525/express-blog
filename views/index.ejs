<%- include('header'); -%>
  <div class="container">
    <h1>Homepage</h1>
    <h2>
      <%= count %>
    </h2>
    <% posts.forEach(function(post) { %>
      <div>
        <a href="/<%= post.slug %>">
          <h2>
            <%= post.title %>
          </h2>
        </a>
      </div>
      <% }) %>
  </div>
  <%- include('footer'); -%>